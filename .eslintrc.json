{
    "env": {
        "browser": true,
        "commonjs": true,
        "es6": true,
        "node": true
    },
    "extends": [
        "airbnb", "prettier"
    ],
    "globals": {
        "Atomics": "readonly",
        "SharedArrayBuffer": "readonly"
    },
    "parserOptions": {
        "ecmaVersion": 2018
    },
    
    "plugins": ["prettier"],
    "rules": {
        "prettier/prettier": ["error"],
        "no-console":0,
        "radix": ["error", "as-needed"],
        "prefer-destructuring": ["error", {"object": false, "array": false}]
    }
}